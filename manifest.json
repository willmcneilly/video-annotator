{
  "name": "Annotator",
  "version": "0.1",
  "description": "An annotator for html5 videos",
  "icons": { "16": "icons/icon16.png",
             "48": "icons/icon48.png",
            "128": "icons/icon128.png" },
  "background": { "scripts": ["src/background.js"] },
  "permissions": [
    "tabs", "http://*/*"
  ],
  "default_locale": "en",
  "browser_action": {
      "name": "Click to change the icon's color"
  },
  "manifest_version": 2
}